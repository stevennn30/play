<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/home">

    <keep_state_fragment
        android:id="@+id/home"
        android:name="com.serafimtech.serafimplay.ui.Navigation_Home_R"
        android:label="home"
        tools:layout="@layout/navigation_rhome">

    </keep_state_fragment>

    <keep_state_fragment
        android:id="@+id/gamepage"
        android:name="com.serafimtech.serafimplay.ui.Navigation_Gamepage"
        android:label="gamepage"
        tools:layout="@layout/navigation_gamepage" />

    <keep_state_fragment
        android:id="@+id/featuredGame"
        android:name="com.serafimtech.serafimplay.ui.Navigation_FeaturedGame"
        android:label="featuredGame"
        tools:layout="@layout/navigation_sfeaturedgame" />

    <keep_state_fragment
        android:id="@+id/ble"
        android:name="com.serafimtech.serafimplay.ui.Navigation_Ble"
        android:label="ble"
        tools:layout="@layout/navigation_ble" />

    <keep_state_fragment
        android:id="@+id/operation"
        android:name="com.serafimtech.serafimplay.ui.Navigation_Operation_R"
        android:label="operation"
        tools:layout="@layout/navigation_operation_r" />

    <keep_state_fragment
        android:id="@+id/mall"
        android:name="com.serafimtech.serafimplay.ui.Navigation_Mall"
        android:label="mall"
        tools:layout="@layout/navigation_mall" />

</navigation>